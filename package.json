{
    "name": "bupa-onlineservices-web",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "dev:ssr": "ng run bupa-onlineservices-web:serve-ssr",
        "serve:ssr": "node ./dist/bupa-onlineservices-web-server/main.js",
        "build:ssr": "ng build --prod --base-href /Bupa.OnlineServices/OnlineServicesWeb/ && ng run bupa-onlineservices-web:server",
        "prerender": "ng run bupa-onlineservices-web:prerender",
        "start": "ng serve --aot",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "build:payments": "ng build --base-href '/AOS/' --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s -d ./doc",
        "mock:server": "npm run concat:mocks && json-server --routes mocks/routes.json --watch mocks/data.json",
        "start:proxy": "ng serve --proxy-config mocks/proxy.conf.json",
        "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
        "concat:mocks": "node \"./mocks/concat-json.js\"",
        "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit"
    },
    "private": true,
    "dependencies": {
